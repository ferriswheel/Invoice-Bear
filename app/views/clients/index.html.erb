<div class="container">
  <div class="row">
    <div class="span10">
<h1>Clients</h1>

<table class="table table-striped">
  <tr>
    <th>Fname</th>
    <th>Lname</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Phone</th>
    <th>Mphone</th>
    <th>Fax</th>
    <th>Email</th>
    <th>Bname</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @clients.each do |client| %>
  <tr>
    <td><%= client.fname %></td>
    <td><%= client.lname %></td>
    <td><%= client.address %></td>
    <td><%= client.city %></td>
    <td><%= client.state %></td>
    <td><%= client.zip %></td>
    <td><%= client.phone %></td>
    <td><%= client.mphone %></td>
    <td><%= client.fax %></td>
    <td><%= client.email %></td>
    <td><%= client.bname %></td>
    <td><%= link_to 'Show', client %></td>
    <td><%= link_to 'Edit', edit_client_path(client) %></td>
    <td><%= link_to 'Destroy', client, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>
</div>

<div class="span2">
<%= link_to 'New Client', new_client_path, :class => "btn btn-primary" %>
</div>

</div>
</div>
